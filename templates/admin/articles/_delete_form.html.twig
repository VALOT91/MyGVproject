<form method="post" action="{{ path('articles_delete', {'id': article.id}) }}" onsubmit="return confirm('Voulez-vous vraiement supprimer cet article ?');">
    <div class="pos-button">
        <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ article.id) }}">
        <button class=" btn text-white buttonStyleC btnBackEnr">Supprimer</button>
    </div>
</form>

