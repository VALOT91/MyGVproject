<form method="post" action="{{ path('product_delete', {'id': product.id}) }}" onsubmit="return confirm('Voulez-vous vraiement supprimer ce produit ?');">
  <div class="pos-button">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ product.id) }}">
    <button class=" btn text-white buttonStyleC btnBackEnr">Supprimer</button>
  </div>
</form>

